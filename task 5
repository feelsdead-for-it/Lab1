#include <iostream>
#include <cmath>
using namespace std;




int main() 
{
    double a,b,S,Dif,M,Div;
    cin >>a>>b;
    
	if (a != 0 && b != 0) 
	{
	    
	    S= abs(a) + abs(b);
		Dif= abs(a) * abs(b);
		M= abs(a) - abs(b);
		Div= abs(a) / abs(b);
		
	cout <<"S= "<<S<<" Dif= "<<Dif<<" M= "<<M<<" Div= "<<Div << ends;
	}
	
	else
	{
		cout << "Incorrect";
	}
}
