#include <iostream>
using namespace std;



int main()
{
	double a,b,S,Dif,M,Div;
	cin >>a>>b;
	if (a != 0 && b != 0) 
	{
	    S= a + b;
	    Dif= a - b;
	    M= a * b;	  
	    Div= (a * a) / (b * b);
	   cout <<"S= "<<S<<" Dif= "<<Dif<<" M= "<<M<<" Div= "<<Div << ends;
	}
	else {
		cout << "Incorrect" << endl;
	}
}	    
